{
  "bindings": [
    {
      "name": "serviceBusTrigger",
      "queueName": "untrusted-file-queue",
      "type": "serviceBusTrigger",
      "direction": "in"
    },
    {
      "name": "storageQueueTrigger",
      "type": "queueTrigger",
      "direction": "in",
      "queueName": "untrusted-file-queue",
      "connection": "AzureWebJobsStorage"
    },
    {
      "name": "storageTrustedQueue",
      "type": "queue",
      "direction": "out",
      "queueName": "trusted-file-queue",
      "connection": "AzureWebJobsStorage"
    },
    {
      "name": "serviceBusTrustedQueue",
      "type": "serviceBus",
      "queueName": "trusted-file-queue",
      "direction": "out"
    },
    {
      "type": "signalR",
      "name": "signalRMessages",
      "hubName": "%AZURE_SIGNALR_HUB_NAME%",
      "connectionStringSetting": "AzureSignalRConnectionString",
      "direction": "out"
    }
  ]
}